[difftool "nvimdiff"]
	cmd = nvim -d $LOCAL $REMOTE
[safe]
	directory = /storage/emulated/0/BelajarJava/maven/belajar-java-thread
	directory = /storage/emulated/0/BelajarJava/maven/belajar-java-stream
	directory = /storage/emulated/0/Documents/Griya Anugrah/Rekap
	directory = /storage/emulated/0/BelajarJava/maven/belajar-java-reflection
