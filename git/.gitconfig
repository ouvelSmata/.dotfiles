[alias]
	cg = config --global
	ko = commit -m
	koma = commit --amend -m
	clso = config --list --show-origin
	lo = log --oneline
	logp = log --oneline --graph
  bl = branch --list
  bc = checkout
  bs = switch
  bsc = branch --show-current
  bd = branch --delete
  br = branch
  sp = stash push -m
  sl = stash list
  sc = stash clear
  sd = stash drop
  sa = stash apply
	logone = log --oneline
[init]
	defaultBranch = main
